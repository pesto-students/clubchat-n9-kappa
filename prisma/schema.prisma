// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id String @id @default(cuid())
  user_name String
  first_name String
  last_name String
  email String
  authetication_metadata String
  created_at String
  updated_at String
}
