// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = "postgres://postgres:javascript@localhost:5432/club_chat"
}

model users {
  id String @id @default(uuid())
  email String @unique
  username String @unique
  hash String
  salt String
  maxage Int
  createdAt Int
}
